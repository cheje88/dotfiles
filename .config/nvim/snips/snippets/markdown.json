{
  "eq": {
    "prefix": ["eq"],
    "body": ["\\$\\$", "$1", "\\$\\$ {#eq-${2: label}}"]
  },
  "Insert Block Equation": {
    "prefix": "$$",
    "body": ["\\$$", "${0}", "\\$$"]
  },
  "img": {
    "prefix": ["img"],
    "body": ["![](./img/$1)"]
  },
  "br": {
    "prefix": ["br"],
    "body": ["<br/>"]
  },
  "todo": {
    "prefix": ["todo"],
    "body": ["* [ ] ${1:task} -- due:${2:today} ${3: project:}"]
  },
  "red": {
    "prefix": ["red"],
    "body": ["<span style=\"color:red\"> $1 </span>"]
  },
  "green": {
    "prefix": ["green"],
    "body": ["<span style=\"color:green\"> $1 </span>"]
  },
  "blue": {
    "prefix": ["blue"],
    "body": ["<span style=\"color:blue\"> $1 </span> $2"]
  },
  "pink": {
    "prefix": ["pink"],
    "body": ["<span style=\"color:pink\"> $1 </span>"]
  },
  "yellow": {
    "prefix": ["yellow"],
    "body": ["<span style=\"color:yellow\"> $1 </span>"]
  },
  "lime": {
    "prefix": ["lime"],
    "body": ["<span style=\"color:lime\"> $1 </span>"]
  },
  "orange": {
    "prefix": ["orange"],
    "body": ["<span style=\"color:orange\"> $1 </span>"]
  }
}
